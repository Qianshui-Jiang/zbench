{
    "$schema": "./_schema.json",

    "resources":
    {
		"query": 
        {
            "initialValues": { "sourcePath": "./flash_decoding_json/tensor_file/q_tensor.npy" }
        },
        "key": 
        {
            "initialValues": { "sourcePath": "./flash_decoding_json/tensor_file/k_tensor.npy" }
        },
        "value": 
        {
            "initialValues": { "sourcePath": "./flash_decoding_json/tensor_file/v_tensor.npy" }
        },
        "output": 
        {
            "initialValuesDataType": "FLOAT16",
            "initialValues": { "valueCount": 4096, "value": 0 }
        }
    },

    "dispatchables":
    {
        "mha": 
        {
            "type": "DML_OPERATOR_MULTIHEAD_ATTENTION",
            "desc": 
            {
                "QueryTensor": { "DataType": "FLOAT16", "Sizes":  [1, 1, 1, 4096] },
                "KeyTensor": { "DataType": "FLOAT16", "Sizes":    [1, 1, 2048, 4096] },
                "ValueTensor": { "DataType": "FLOAT16", "Sizes":  [1, 1, 2048, 4096] },
                "OutputTensor": { "DataType": "FLOAT16", "Sizes": [1, 1, 1, 4096] },
                "Scale": 0.0883883,
                "MaskFilterValue": -10000000,
                "HeadCount": 32,
                "MaskType": "DML_MULTIHEAD_ATTENTION_MASK_TYPE_NONE"
            },
            "executionFlags":[
                "DML_EXECUTION_FLAG_ALLOW_HALF_PRECISION_COMPUTATION"
            ]
        }
    },

    "commands":
    [
        {
            "type": "dispatch",
            "dispatchable": "mha",
            "bindings": 
            {
                "QueryTensor": "query",
                "KeyTensor": "key",
                "ValueTensor": "value",
                "OutputTensor": "output"
            }
        },
		{    "type": "writeFile", "targetPath": "./flash_decoding_json/tensor_file/dml_mha_q_k_v2048_query.npy", "resource": "query"},
		{    "type": "writeFile", "targetPath": "./flash_decoding_json/tensor_file/dml_mha_q_k_v2048_key.npy", "resource": "key"},
		{    "type": "writeFile", "targetPath": "./flash_decoding_json/tensor_file/dml_mha_q_k_v2048_value.npy", "resource": "value"},
		{    "type": "writeFile", "targetPath": "./flash_decoding_json/tensor_file/dml_mha_q_k_v2048_output.npy", "resource": "output"} 
    ]
}